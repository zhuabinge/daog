.dealbox .deal .con:hover{border-color:#f10582;}
.dealbox .deal .con .title{height: 40px; line-height: 20px; padding: 0 10px; margin:5px 0; overflow: hidden; clear: both; position:relative; font-size:14px;}
.dealbox .deal .con .title span{position: absolute; top:0; left:5px;}

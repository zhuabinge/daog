.con .price .price-right{position:absolute; right:14px; top:3px;}
.price-right a{display:block; position:absolute; right:0;}
.price-right .to-taobao{background-position:-175px -229px; width:20px; height:21px;}
.price-right .to-tianmao{background-position:-145px -229px; width:24px; height:18px;}
.price-right > span{display:block; margin-top:20px;}
.price .product-buy{position:absolute; top:0; right:0;}
.product-buy a.btn-buy{display:none; position:absolute; right:-80px; top:0; background-position:-104px -259px; width:98px; height:38px; line-height:41px; font-size:15px; font-weight:bold; color:#fff; text-decoration:none;}
.product-buy > span{display:none; position:absolute; right:0; top:0; background-position: -202px -259px; width:5px; height:38px;}
.product-buy .btn-buy span{padding-left:16px;}
.newicon{position:absolute; left:-1px; top:-6px; width:55px; height:48px; overflow:hidden; background-position:-214px -88px;}

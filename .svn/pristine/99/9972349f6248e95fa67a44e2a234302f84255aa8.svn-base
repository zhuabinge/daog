.rule-list ul li a{text-decoration:none;}
.rule-list .fr{padding:25px 0 0 50px;}
.rule-list .hei-rule{display:block; background:url(../images/seller-center/rule_hei.jpg) no-repeat; width:183px; height:103px;}
/*规则中心详细内容*/
.rule-01 h2, .rule-02 h2{height:45px; line-height:45px; font-size:19px; text-align:center; color:#222; background-color:#e8e8e8;}
.rule-01 a:hover{color:#ff5151; text-decoration:none;}
.rule-02 a:hover{color:#55b1ae; text-decoration:none;}
.rule-toggle{padding:20px 0;}
.rule-toggle > li{padding:5px 0 5px 25px; line-height:30px;}
.rule-toggle > li > i{display:inline-block; width:13px; height:13px; cursor:pointer; margin-right:5px;}
.rule-toggle > li > i.close{background-position:-305px -209px;}
.rule-toggle > li > i.open{background-position:-324px -209px;}
.rule-toggle > li > a{font-size:16px; font-weight:bold;}
.rule-toggle > li > a.active{color:#ff5151;}
.rule-toggle > li > ul{display:none; padding:5px 0;}
.rule-toggle > li > ul > li{padding-left:10px; line-height:30px; font-size:14px;}
.rule-toggle > li > ul > li a:hover{color:#55b1ae;}
.rule-toggle em{display:inline-block; background-position:-293px -214px; width:6px; height:8px; margin-right:3px;}
.rule-02 ul{padding:20px 0;}
.rule-02 ul li{font-size:16px; padding:5px 0 5px 25px;}

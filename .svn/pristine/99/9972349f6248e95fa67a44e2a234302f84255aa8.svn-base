/*规则中心*/
.container.nb{border:none; background-color:#fafafa;}
.rule-section{margin:30px 0;}
.rule-title, .rule-content{clear:both; overflow:hidden;}
.rule-title > h3{float:left; height:30px; line-height:30px; background-color:#ff5151; font-size:15px; color:#fff; padding: 0 20px;}
.rule-title > span{float:left; display:block; background-position:-275px -192px; width:10px; height:30px;}
.rule-box1{padding:40px 70px; border:1px solid #dbdbdb; background-color:#fff;}
.rule-pic1{background:url(../images/seller-center/rule1.png) no-repeat; width:839px; height:128px; position:relative;}
.rule-pic1 a, .rule-pic2 a{text-indent:-9999px; height:122px; width:85px; position:absolute; top:0;}
.rule-pic1 .r1{left:0;}
.rule-pic1 .r2{left:190px;}
.rule-pic1 .r3{left:378px;}
.rule-pic1 .r4{left:566px;}
.rule-pic1 .r5{left:754px;}
.rule-box2{float:left; padding:40px 60px; margin-right:20px; border:1px solid #dbdbdb; background-color:#fff;}
.rule-pic2{background:url(../images/seller-center/rule2.png) no-repeat; width:287px; height:128px; position:relative;}
.rule-list{float:left; width:488px; height:168px; padding:20px 30px; margin-bottom:20px; border:1px solid #dbdbdb; background-color:#fff;}

/*
 * 天天逛逛手机网页样式
 * 版权所有 ttgg.com
 * @webuffer 2014.12
 */
html, body, div, p, ul, ol, li, dl, dt, dd, form, input, table, img, h1, h2, h3, h4, h5, h6, header, section, footer, nav, figure, article, figcaption{margin:0; padding:0;}
body{min-width:320px; background-color:#eee; font-family:"微软雅黑", arial; font-size:12px; color:#6e6e6e;}
img{border:none;}
a{text-decoration:none; color:#6e6e6e;}
.clear{clear:both;}
.clearfix{clear:both; overflow:hidden;}
#backTop i, header h1 a, .search, .big-nav i, .deal i, .back, .search-form .smt, .title-bar .arrow-down, .dropdown-menu .arrow-up, 
.tags ul li sup, .new .title i, .prefers .add, .opt-group i, .confirm i, .btn-below i{display:block; background:url(../images/m_ttgg.png) no-repeat; background-size:116px;}
#backTop{display:block; width:35px; height:35px; position:fixed; bottom:65px; right:15px; z-index:1000; background-color:rgba(241,5,130,0.7); border-radius:50%;}
#backTop i{background-position:-48px -33px; width:16px; height:25px; margin:5px 10px;}
header{background-color:#f10582; position:relative; overflow:hidden; min-width:300px; height:28px; padding:10px 10px; text-align:center;}
header h1{font-size:1.5em; color:#fff; font-weight:normal; line-height:28px;}
header h1 a{background-position:0 0; width:83px; height:28px; margin:0 auto;}
#wrapper .search{background-position:-49px -85px; width:14px; height:15px; position:absolute; right:15px; top:16px;}
.addWrap{position:relative; width:100%; background:#fff;}
.addWrap .swipe{overflow:hidden; visibility:hidden; position:relative;}
.addWrap .swipe-wrap{overflow:hidden; position:relative;}
.addWrap .swipe-wrap > div{float:left; width: 100%; position:relative;}
.addWrap .swipe-wrap img{display:block; max-width:100%; height:auto;}
#position{position:absolute; bottom:0; left:0; padding-right:8px; margin:0; width:100%; text-align:center;}
#position li{width:8px; height:8px; margin:0 2px; display:inline-block; border-radius:50%; background-color:#AFAFAF;}
#position li.cur{background-color:#ff5500;}
.big-nav{width:100%; height:60px; padding:10px 0; margin-bottom:10px; background-color:#fff; border-bottom:1px solid #d5d5d5; clear:both; overflow:hidden;}
.big-nav a{float:left; display:block; width:25%; text-align:center;}
.big-nav em{display:block; width:40px; height:28px; padding:6px 0; margin:2px auto; border-radius:50%; background-color:#f10582;}
.big-nav .qiandao em{background-color:#f10582;}
.big-nav .nanzhuang em{background-color:#409dd8;}
.big-nav .xiebao em{background-color:#7375b6;}
.big-nav .more em{background-color:#ff7d00;}
.big-nav i{margin:0 auto;}
.big-nav .qiandao i{background-position:-9px -73px; width:21px; height:24px;}
.big-nav .xiebao i{background-position:-9px -99px; width:21px; height:26px;}
.big-nav .nanzhuang i{background-position:-9px -127px; width:21px; height:26px;}
.big-nav .more i{background-position:-9px -153px; width:21px; height:23px;}
#wrapper .banner{padding:5px 5px; margin-bottom:10px; border-top:1px solid #d5d5d5; border-bottom:1px solid #d5d5d5; background-color:#fff;}
.banner-con{width:100%; clear:both; overflow:hidden;}
.banner-con .fl{width:40.7%; float:left;}
.banner-con .fr{width:59.3%; float:right;}
.banner-con .frt{width:100%; float:left;}
.banner-con .frb1, .banner-con .frb2{width:50%; float:left;} 
.banner-con .fl .b-inner{border-right:1px solid #ddd;}
.banner-con .frt .b-inner{border-bottom:1px solid #ddd;}
.banner-con .frb1 .b-inner{border-right:1px solid #ddd;}
.banner-con img{width:100%; height:auto;}
#wrapper .deals{clear:both; overflow:hidden; padding-left:8px;}
.deals .deal{width:50%; float:left;}
.deals .deal-box{padding:0 10px 0 0;}
.deals .deal-con{width:100%; position:relative; padding-bottom:1px; margin:0 auto 10px auto; border:1px solid #d5d5d5; background-color:#fff;}
.deal-con figure img{width:100%; height:auto;}
.deal-con figure sup{position:absolute; top:0; left:0;}
.deal-con .icon-tm{background-position:-82px -130px; width:12px; height:12px;}
.deal-con .icon-tb{background-position:-82px -111px; width:12px; height:12px;}
.deal-con figure sub{position:absolute; right:3px; bottom:52px;}
.deal-con figure sub a{display:block; width:13px; height:11px; padding:7px 6px; border-radius:50%; background-color:rgba(255,255,255,0.6);}
.deal-con figure sub i{background-position:-50px -122px; width:13px; height:11px;}
.deal-con .icon-lvred{background-position:-50px -106px; width:13px; height:11px;}
.deal-con h2{font-size:1.0em; font-weight:normal; height:20px; line-height:20px; overflow:hidden; padding:0 5px;}
.deal-con h2 a{display:inline-block; position:relative;}
.deal-con h2 a em{position:absolute; left:-8px; top:0; font-style:normal;}
.deal-con p{padding:3px 5px;}
.deal-con p span{color:#f10582; font-size:1.2em; font-weight:300;}
.deal-con p span small{font-size:0.8em;}
.deal-con p del{font-size:1.0em;}
.deal-con .count{display:block; font-size:10px; color:#6e6e6e;}
/**分类及分类页**/
.back{background-position:-51px -159px; width:10px; height:18px; position:absolute; left:10px; top:15px;}
.search-form{height:28px; padding-left:20px;}
.search-form .s-outer{width:100%; position:relative; background-color:#fff; border-radius:15px;}
.search-form .s-inner{padding:0 15px 0 30px;}
.search-form .smt{position:absolute; left:10px; top:7px; background-position:-49px -64px; width:15px; height:15px; border:none; text-indent:-300px;}
.search-form .txt{width:100%; height:28px; font-family:"微软雅黑",arial; color:#6e6e6e; border:none; outline:none;} 
.sf1{padding:0 40px 0 0;}
.s-mask{width:100%; height:100%; position:absolute; top:0; left:0; background-color:rgba(0,0,0,0.6); z-index:10;}
.nav3{padding:20px 0; margin-bottom:0; border-bottom:none;}
.nav3 a{width:33.33%;}
.big-nav .c99 em{background-color:#ff4e49;}
.big-nav .c20 em{background-color:#0bbdfd;}
.big-nav .c99 i{background-position:-5px -27px; width:29px; height:23px;}
.big-nav .c20 i{background-position:-7px -49px; width:25px; height:23px;}
.cate-nav{width:100%; background-color:#fff; clear:both; overflow:hidden;}
.cate-box{width:33.33%; float:left;}
.cate-nav a{display:block; position:relative; width:97px; height:120px; margin:5px auto 10px auto; text-align:center;}
.cate-nav img{width:97px; height:98px; margin-bottom:2px;}
.cate-nav .mask{display:block; position:absolute; top:0; left:0; z-index:5; background:url(../images/cate_mask.png) no-repeat; background-size:97px; width:97px; height:98px;}
.title-bar{position:relative; margin-bottom:10px; height:20px; padding:10px 0; width:100%; background-color:#fff; border-bottom:1px solid #d5d5d5; font-size:14px;}
.title-bar .all{float:left; width:33.33%;}
.title-bar .recent-select{float:right; width:66.66%; margin-top:-19px;}
.title-bar > ul{clear:both; overflow:hidden; list-style-type:none;}
.title-bar > ul > li{float:left; text-align:center;}
.title-bar .all > li{width:100%;}
.title-bar .recent-select > li{width:50%;}
.title-bar .arrow-down{display:inline-block; background-position:-47px -183px; width:18px; height:7px;}
.dropdown-menu{display:none; position:absolute; top:40px; left:8.33%; z-index:5; width:20%; max-width:140px; padding:15px 3px 0 3px; background-color:rgba(0,0,0,0.7); border-radius:5px;}
.dropdown-menu .arrow-up{position:absolute; top:-5px; left:50%; margin-left:-7px; background-position:-82px -163px; width:14px; height:5px;}
.dropdown-menu ul{list-style-type:none; display:block;}
.dropdown-menu ul li{display:block; text-align:center; margin-bottom:15px;}
.dropdown-menu a{display:block; width:auto; height:auto; color:#fff;}
/**选择我的偏好**/
.head-fr{position:absolute; right:10px; top:13px; color:#fff; font-size:1.3em;}
.tags ul{clear:both; overflow:hidden; list-style-type:none; margin-left:10px;}
.tags ul li{float:left; position:relative; width:69px; height:35px; margin:10px 5px 0 0; text-align:center; line-height:35px; border:1px solid #d5d5d5; background-color:#fff;}
.tags ul li sup{display:block; position:absolute; top:0; right:0; background-position:-83px -146px; width:9px; height:8px;}
.tags ul li a{font-size:1.1em;}
.tags .selected{border-color:#999;}
.tags .selected a{color:#3b3b3b;}
.info-b{padding:10px 0; margin:20px 0; font-size:1.2em; text-align:center; color:#e5e5e5; background-color:#7f7f7f;}
/**今日上新与有好货**/
.news{min-width:320px;}
.news .newsbox{padding:10px 10px;}
.newsbox .new{background-color:#fff; margin-bottom:10px;}
.new figure{width:100%; position:relative;}
.new figure img{width:100%; height:auto;}
.new figure sub{position:absolute; bottom:15px; right:0; height:30px; line-height:30px; background-color:rgba(255,255,255,0.7); border-top-left-radius:15px; border-bottom-left-radius:15px; font-size:1.4em; color:#f10582; padding:0 10px;} 
.new figure sub small{font-size:0.8em;}
.new .title{clear:both; overflow:hidden; min-width:272px; padding:5px 20px 0 8px; position:relative;}
.new .title h2{font-size:1.2em; font-weight:normal; width:100%; height:20px; line-height:20px; overflow:hidden;}
.new .title h2 a{display:inline-block; position:relative;}
.new .title h2 a em{position:absolute; left:-8px; top:0; font-style:normal;}
.new .title h2 a span{padding-left:3.5em;}
.new .title i{position:absolute; right:5px; top:9px;}
.new .title .icon-tm{background-position:-82px -130px; width:12px; height:12px;}
.new .title .icon-tb{background-position:-82px -111px; width:12px; height:12px;}
.new > p{padding:8px 8px; font-size:1.1em; line-height:18px; height:46px; overflow:hidden;}
.new .rel{padding:8px 8px;}
.new .rel span{font-size:1.2em; padding-right:5px;}
.new .rel a{display:inline-block; height:20px; line-height:20px; padding:0 10px; margin:0 5px 5px 0; border:1px solid #f10582; border-radius:10px; color:#f10582;}
.prefers{position:relative; height:40px; overflow:hidden; padding:0 10px; background-color:#fff;}
.prefers a{font-size:1.2em; line-height:38px;}
.prefers > a{position:absolute;}
.prefers .scroll{padding:0 45px; clear:both; overflow:hidden;}
.prefers .scroll a{float:left; width:25%; text-align:center;}
.prefers .add{right:10px; top:11px; background-position:-48px -138px; width:17px; height:17px;}
.prefers .active{color:#f10582; border-bottom:2px solid #f10582;}
/**登录注册**/
.login{padding:15px 15px;}
.login .title{position:relative; margin:25px 0; border-bottom:1px solid #d8d8d9;}
.login .title h2{position:absolute; top:-12px; left:50%; width:150px; height:24px; line-height:24px; margin-left:-75px; text-align:center; font-size:1.3em; font-weight:normal; background-color:#eee;}
.login .t-blow h2{font-size:1.2em;}
.login input{font-family:"微软雅黑", arial; border:none; outline:none; color:#6e6e6e;}
.login .btn{width:100%; height:40px; line-height:40px; margin-top:10px; background-color:#f10582; border-radius:2px; text-align:center; color:#fff; font-size:1.3em;}
.login .opt-group{position:relative; padding-left:30px; background-color:#fff; border-bottom:1px solid #ededed;}
.opt-group .opt{height:40px; padding-right:10px;}
.opt-group label{display:block; position:absolute; left:0; top:0; width:30px; height:40px;}
.opt-group label i{margin:15px auto;}
.opt-group .lg-name i{background-position:-82px -46px; width:11px; height:11px;}
.opt-group .lg-pwd i{background-position:-82px -61px; width:11px; height:11px;}
.opt-group .lg-email i{background-position:-81px -31px; width:13px; height:11px;}
.opt-group .opt input{width:100%; height:24px; padding:8px 0;}
.login .p-right{text-align:right; padding:10px 0;}
.login .btn-below{text-align:center;}
.btn-below a{display:block; width:75px; height:67px; margin:0 auto; padding-top:8px; background-color:#0f97e1; border-radius:50%; color:#fff; font-size:1.2em;}
.btn-below i{background-position:-4px -178px; width:31px; height:35px; margin:0 auto;}
.code{clear:both; overflow:hidden; margin-top:10px;}
.code .opt-box{float:left; width:50%; height:40px; background-color:#fff;}
.code .opt{padding:0 10px;}
.code .opt input{width:100%; height:24px; margin-top:8px; text-align:center;}
.code .code-pic{float:right; width:50%;}
.code-pic .pic-box{padding-left:10px;}
.code-pic img{width:100%; height:40px;}
.confirm{margin:15px 0 5px 0;}
.confirm a{color:#f10582;}
.confirm .checkbox{cursor:pointer;}
.confirm input{-webkit-appearance:none; appearance:none;}
.confirm i{display:inline-block; background-position:-82px -77px; width:12px; height:12px; vertical-align:middle; margin-right:3px; margin-top:-1px;}
.confirm .active{background-position:-82px -94px;}